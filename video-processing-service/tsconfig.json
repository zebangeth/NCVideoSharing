{
    "compilerOptions": {
        "target": "es6", 
        "module": "commonjs",
        "rootDir": "src",
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true},
    "include": ["src/**/*.ts"], // build all ts files in src folder
    "exclude": ["node_modules"] // exclude node_modules folder
}
